/* pages/goTovideofabu/goTovideofabu.wxss */
page{
  background: #e4e4e4
}
.text{
  margin-top: 20rpx;
  margin-left: 50rpx;
  font-size: 36rpx
}
.input{
  background: #fff;
  margin: 20rpx auto;
  width: 650rpx;
  height: 400rpx;
  border-radius: 20rpx;
  border: 2rpx solid #F6BC41
}
.footer{
  display: flex;
  width: 100%;
  background-color: #fff;
  position: fixed;
  z-index: 99;
  justify-content: center;
  bottom: 0

}
button{
  background-color: #F6BC41;
  width: 200rpx;
  height: 100rpx
}

/* 图片样式 */
.image-list {
  display: flex;
  flex-wrap: wrap;
  margin-left: 50rpx;
  margin-top: 20rpx
}
.video{
position: relative;
  overflow: hidden;
  text-align: center;
}
.video-wrap {
  background: #fff;
  width: 200rpx;
  height: 200rpx;
  position: relative;
  overflow: hidden;
  text-align: center;
}
.selectphoto {
  border: 2rpx dashed #F6BC41;
  position: relative;
}
.icon-add-fill {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  color: #F6BC41;
  font-size: 60rpx;
}
.video{
  position: relative
}
.icon-wrong {
  position: absolute;
  top: 0;
  right: 0;
  width: 50rpx;
  height: 50rpx;
  background-color: #000;
  opacity: 0.4;
  color: #fff;
  text-align: center;
  line-height: 50rpx;
  font-size: 48rpx;
  font-weight: bolder;
}